{
  "name": "dilsaycare-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:local": "bash scripts/start-local.sh",
    "start:stage": "WEB_URL=https://dev.dilsaycare.in expo start",
    "start:prod": "WEB_URL=https://dilsaycare.in expo start",
    "ios:local": "bash scripts/start-local.sh --ios",
    "ios:stage": "WEB_URL=https://dev.dilsaycare.in expo start --ios",
    "android:local": "bash scripts/start-local.sh --android",
    "android:stage": "WEB_URL=https://dev.dilsaycare.in expo start --android",
    "update:dev:ip": "node scripts/update-dev-ip.js",
    "build:dev": "eas build --profile development --platform android",
    "build:staging": "eas build --profile staging --platform android",
    "build:prod": "eas build --profile production --platform android",
    "build:ios:staging": "eas build --profile staging --platform ios",
    "build:ios:prod": "eas build --profile production --platform ios"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "expo-asset": "^12.0.9",
    "expo-constants": "~17.0.8",
    "expo-file-system": "^19.0.17",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-razorpay": "^2.3.1",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  },
  "private": true
}
